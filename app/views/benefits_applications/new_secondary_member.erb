<%= form_with model: @form, url: { action: :create_member }, method: :post, local: true, builder: FoundationalProjectFormBuilder, class: "form-card" do |f| %>
  <h2>Tell us about your dependent</h2>
  <%= f.vita_min_text_field(:first_name, "What's your dependents first name?") %>
  <%= f.vita_min_text_field(:last_name, "What's your dependents last name?") %>
  <%= f.vita_min_text_field(:date_of_birth, "What's your dependents birthdate?", options: {type: "date"}) %>
  <%= render 'benefits_applications/member_list', locals: {members: @members} %>
  <%= f.button('Add Member â†’', type: :submit, formaction: create_member_path, class: "button spacing-above-60") %>
  <%= f.button('Continue â†’', type: :submit, formaction: validate_application_path, class: "button button--wide button--primary spacing-above-60") %>
<% end %>