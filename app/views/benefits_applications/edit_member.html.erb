<%= form_with model: @primary_member_form, url: { action: :create_member }, method: :patch, local: true, builder: FoundationalProjectFormBuilder, class: "form-card" do |f| %>
  <h2>Edit Member</h2>
  <%= f.vita_min_text_field(:first_name, "What's your first name?") %>
  <%= f.vita_min_text_field(:last_name, "What's your last name?") %>
  <%= f.vita_min_text_field(:date_of_birth, "What's your birthdate?", options: {type: "date"}) %>
  <%= render 'benefits_applications/member_list', locals: {members: @members} %>
  <%= f.button('Edit Member â†’', type: :submit, formaction: create_member_path, class: "button spacing-above-60") %>
<% end %>