<table class="data-table">
  <tbody>
  <% @members.each do |member| %>
    <tr>
      <td><%= member.full_name %></td>
      <td>
        <%= link_to "Edit", edit_member_path(member) %>
        <% unless member.is_primary %>&nbsp;<%= link_to "Delete", delete_member_path(member) %><% end %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>